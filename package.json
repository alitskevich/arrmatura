{
  "name": "arrmatura",
  "version": "3.0.1",
  "description": "framework",
  "scripts": {
    "start": "npx webpack-dev-server --hot",
    "build": "npx webpack-cli",
    "share-grodno": "cp docs/index.js* ~/grodno/vendor/armatura",
    "share-dma": "cp docs/index.js* ~/aimd-dma-client/vendor/armatura"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alitskevich/arrmatura.git"
  },
  "author": "alitskevich",
  "license": "MIT",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-loader": "^8.0.6",
    "raw-loader": "^4.0.0",
    "standard": "^14.3.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "standard": {
    "globals": [
      "console"
    ],
    "ignore": [
      "node_modules/**"
    ],
    "plugins": [
      "html"
    ],
    "parser": "babel-eslint",
    "envs": [
      "jest"
    ],
    "validate": [
      "javascript",
      "javascriptreact",
      {
        "language": "html",
        "autoFix": true
      }
    ]
  },
  "dependencies": {
    "eslint": "^6.8.0"
  }
}
